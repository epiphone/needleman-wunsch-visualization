{
  "name": "needleman-wunsch-visualization",
  "version": "1.0.0",
  "description": "Visualizing the Needleman-Wunsch algorithm; coursework for a bioinformatics class",
  "main": "index.js",
  "scripts": {
    "build": "browserify src/main.js -o src/js/bundle.js",
    "watch": "watchify src/main.js -o src/js/bundle.js -v",
