  "bugs": {
    "url": "https://github.com/epiphone/needleman-wunsch-visualization/issues"
  },
  "homepage": "https://github.com/epiphone/needleman-wunsch-visualization",
  "dependencies": {
    "browserify": "^9.0.8",
    "d3": "^3.5.5",
    "jquery": "^2.1.3",
    "underscore": "^1.8.3",
    "watchify": "^3.1.1"
  }
}
